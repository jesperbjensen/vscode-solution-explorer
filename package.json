    "license": "CC-BY-SA-4.0",
    "publisher": "jesperbjensen",
    "icon": "images/icon.png",
    "author": {
        "email": "jesperbjensen@gmail.com",
        "name": "Jesper Blad Jensen",
        "url": "https://github.com/jesperbjensen"
    },
    "repository": {
        "type": "git",
